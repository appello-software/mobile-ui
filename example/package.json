{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ios:dev": "ENVFILE=.app-config/dev/.env react-native run-ios --terminal=zsh",
    "ios:stage": "ENVFILE=.app-config/stage/.env react-native run-ios",
    "ios:prod": "ENVFILE=.app-config/prod/.env react-native run-ios",
    "ios:run-build": "git tag ios-build -f -m \"[ios build]\" && git push --tag --force",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:dev": "react-native run-android --variant=devDebug",
    "android:stage": "react-native run-android --variant=stageDebug",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:build-dev": "cd android && ./gradlew assembleDev",
    "android:build-stage": "cd android && ./gradlew assembleStage",
    "android:build-prod": "cd android && ./gradlew assembleProd",
    "android:bundle": "cd android && ./gradlew bundleProd",
    "android:run-build": "git tag android-build -f -m \"[android build]\" && git push --tag --force",
    "run-builds": "git tag android-build -f -m \"[android build]\" && git tag ios-build -f -m \"[ios build]\" && git push --tag --force",
    "test": "jest"
  },
